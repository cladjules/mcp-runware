{
  "name": "mcp-runware",
  "version": "1.0.0",
  "description": "MCP server for Runware AI image generation",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-runware": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && cp -r src/data dist/",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "tsc && cp -r src/data dist/ && node dist/index.js",
    "inspect": "npx @modelcontextprotocol/inspector",
    "start:http": "MCP_TRANSPORT=http node dist/index.js",
    "fetch-models": "python3 scripts/fetch_curated_models.py"
  },
  "keywords": [
    "mcp",
    "runware",
    "ai",
    "image-generation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@runware/sdk-js": "^1.2.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.19.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.2",
    "@vercel/node": "^5.5.28",
    "typescript": "^5.7.2"
  }
}
